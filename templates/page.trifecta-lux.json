    "feature_with_image_bLrJJw": {
      "type": "feature-with-image",
      "blocks": {
        "subtitle_yRF8iA": {
          "type": "subtitle",
          "settings": {
            "text": "Why Lux is Critical for Professional Lighting:",
            "color": "#bc7c08",
            "font_size": 18,
            "font_weight": "700",
            "text_align": "left",
            "max_width": 1200,
            "padding_top": 0,
            "padding_bottom": 0,
            "padding_left": 0,
            "padding_right": 0
          }
        },
        "title_pWyMkA": {
          "type": "title",
          "settings": {
            "text": "At The Makeup Light (TML), our lights are engineered to provide a surface lux reading of 67,000 and project light up to 5 feet. This means that even when the light source is farther from the subject, it still delivers a substantial and evenly distributed amount of light to illuminate the scene without creating harsh shadows or hotspots. This kind of control is crucial in professional settings where consistent lighting quality is necessary across different environments.\u2028\u2028.",
            "color": "#333333",
            "font_size": 18,
            "font_weight": "400",
            "text_align": "left",
            "max_width": 1000,
            "padding_top": 0,
            "padding_bottom": 0,
            "padding_left": 0,
            "padding_right": 0
          }
        },
        "subtitle_jWFPRw": {
          "type": "subtitle",
          "settings": {
            "text": "TML’s Sidelight Technology and Lightguide:",
            "color": "#bc7c08",
            "font_size": 18,
            "font_weight": "700",
            "text_align": "left",
            "max_width": 1200,
            "padding_top": 0,
            "padding_bottom": 0,
            "padding_left": 0,
            "padding_right": 0
          }
        },
        "title_dkE9GN": {
          "type": "title",
          "settings": {
            "text": "The lightguide refracts and diffuses the light, dispersing it evenly across a wider area. This increases the projection and diffusion of the light, this allows the light to hit the subject without harming the retina of their eye.",
            "color": "#333333",
            "font_size": 18,
            "font_weight": "400",
            "text_align": "left",
            "max_width": 1000,
            "padding_top": 0,
            "padding_bottom": 0,
            "padding_left": 0,
            "padding_right": 0
          }
        },
        "image_9FwBgD": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/0562t-ClickCon24-097-1-CT_3.png",
